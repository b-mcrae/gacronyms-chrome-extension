{
  "manifest_version": 2,
  "name": "gAcronyms",
  "description": "This extension makes it easy to search for Genentech and Roche acronyms.",
  "version": "4.1",
  "permissions": [
    "http://*.gene.com/"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "icons": {
       "16":  "images/icon_128.png",
			 "48":  "images/icon_128.png",
			 "128": "images/icon_128.png"
  },
  "browser_action": {
    "default_icon": "images/icon_128.png",
    "default_popup": "app.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+A",
        "mac": "Alt+A",
        "chromeos": "Alt+A",
        "linux": "Alt+A"
      }
    }
  }
}
